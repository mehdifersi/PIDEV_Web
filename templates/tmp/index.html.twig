<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
    {# Run composer require symfony/webpack-encore-bundle to start using Symfony UX #}
    {% block stylesheets %}
        <!-- Google Font -->
        <link href="https://fonts.googleapis.com/css2?family=Cairo:wght@200;300;400;600;900&display=swap" rel="stylesheet">

        <!-- Css Styles -->
        <link rel="stylesheet" href="{{ asset('hela/front/css/bootstrap.min.css') }}" type="text/css">
        <link rel="stylesheet" href="{{ asset('hela/front/css/font-awesome.min.css') }}" type="text/css">
        <link rel="stylesheet" href="{{ asset('hela/front/css/elegant-icons.css') }}" type="text/css">
        <link rel="stylesheet" href="{{ asset('hela/front/css/nice-select.css') }}" type="text/css">
        <link rel="stylesheet" href="{{ asset('hela/front/css/jquery-ui.min.css') }}" type="text/css">
        <link rel="stylesheet" href="{{ asset('hela/front/css/owl.carousel.min.css') }}" type="text/css">
        <link rel="stylesheet" href="{{ asset('hela/front/css/slicknav.min.css') }}" type="text/css">
        <link rel="stylesheet" href="{{ asset('hela/front/css/style.css') }}" type="text/css">
    {% endblock %}

    {% block javascripts %}
        <script src="{{ asset('hela/front/js/jquery-3.3.1.min.js') }}"></script>
        <script src="{{ asset('hela/front/js/bootstrap.min.js') }}"></script>
        <script src="{{ asset('hela/front/js/jquery.nice-select.min.js') }}"></script>
        <script src="{{ asset('hela/front/js/jquery-ui.min.js') }}"></script>
        <script src="{{ asset('hela/front/js/jquery.slicknav.js') }}"></script>
        <script src="{{ asset('hela/front/js/mixitup.min.js') }}"></script>
        <script src="{{ asset('hela/front/js/owl.carousel.min.js') }}"></script>
        <script src="{{ asset('hela/front/js/main.js') }}"></script>
    {% endblock %}
    <!-- Page Preloder -->
    <div id="preloder">
        <div class="loader"></div>
    </div>

    <!-- Humberger Begin -->
    <div class="humberger__menu__overlay"></div>
    <div class="humberger__menu__wrapper">
        <div class="humberger__menu__logo">
            <a href="#"><img src="img/logo.png" alt=""></a>
        </div>
        <div class="humberger__menu__cart">
            <ul>
                <li><a href="#"><i class="fa fa-heart"></i> <span>1</span></a></li>
            </ul>
            <div class="header__cart__price">item: <span>$150.00</span></div>
        </div>
        <div class="humberger__menu__widget">
            <div class="header__top__right__language">
                <img src="img/language.png" alt="">
                <div>English</div>
                <span class="arrow_carrot-down"></span>
                <ul>
                    <li><a href="#">French</a></li>
                    <li><a href="#">English</a></li>
                </ul>
            </div>
            <div class="header__top__right__auth">
                <a href="#"><i class="fa fa-user"></i> Login</a>
            </div>
        </div>
        <nav class="humberger__menu__nav mobile-menu">
            <ul>
                <li class="active"><a href="./index.html">Accueil</a></li>
                <li><a href="./shop-grid.html">Shop</a></li>
                <li><a href="#">Pages</a>
                    <ul class="header__menu__dropdown">
                        <li><a href="./shop-details.html">Commande</a></li>
                        <li><a href="./shoping-cart.html">Panier</a></li>
                        <li><a href="./checkout.html">Check Out</a></li>
                        <li><a href="./blog-details.html">Produit Details</a></li>
                    </ul>
                </li>
                <li><a href="./blog.html">Notre Blog</a></li>
                <li><a href="./contact.html">Contact</a></li>
            </ul>
        </nav>
        <div id="mobile-menu-wrap"></div>
        <div class="header__top__right__social">
            <a href="#"><i class="fa fa-facebook"></i></a>
            <a href="#"><i class="fa fa-twitter"></i></a>
            <a href="#"><i class="fa fa-linkedin"></i></a>
            <a href="#"><i class="fa fa-pinterest-p"></i></a>
        </div>
        <div class="humberger__menu__contact">
            <ul>
                <li><i class="fa fa-envelope"></i> contact@vivalavilla.tn</li>
                <li>Meilleur agence immobiliere</li>
            </ul>
        </div>
    </div>
    <!-- Humberger End -->

    <!-- Header Section Begin -->
    <header class="header">
        <div class="header__top">
            <div class="container">
                <div class="row">
                    <div class="col-lg-6 col-md-6">
                        <div class="header__top__left">
                            <ul>
                                <li><i class="fa fa-envelope"></i> contact@vivalavilla.tn</li>
                                <li>Meilleur agence immobiliere</li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-lg-6 col-md-6">
                        <div class="header__top__right">
                            <div class="header__top__right__social">
                                <a href="#"><i class="fa fa-facebook"></i></a>
                                <a href="#"><i class="fa fa-twitter"></i></a>
                                <a href="#"><i class="fa fa-linkedin"></i></a>
                                <a href="#"><i class="fa fa-pinterest-p"></i></a>
                            </div>
                            <div class="header__top__right__language">
                                <img src="img/language.png" alt="">
                                <div>English</div>
                                <span class="arrow_carrot-down"></span>
                                <ul>
                                    <li><a href="#">French</a></li>
                                    <li><a href="#">English</a></li>
                                </ul>
                            </div>
                            {% if app.user %}
                                <div class="mb-3">
                                    You are logged in as {{ app.user.userIdentifier }}, <a href="{{ path('app_logout') }}">Logout</a>
                                </div>
                            {% endif %}
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="container">
            <div class="row">
                <div class="col-lg-3">
                    <div class="header__logo">
                        <a href="./index.html"><img src="img/logo.png" alt=""></a>
                    </div>
                </div>
                <div class="col-lg-6">
                    <nav class="header__menu">
                        <ul>
                            <li class="active"><a href="./index.html">accueil</a></li>
                            <li><a href="{{ path('app_contrat_front', {'id': app.user.id})}}">Mes Contrats</a></li>
                            <li><a href="#">Pages</a>
                                <ul class="header__menu__dropdown">
                                    <li><a href="./shop-details.html">Produit Details</a></li>
                                    <li><a href="./shoping-cart.html">panier</a></li>
                                    <li><a href="./checkout.html">Check Out</a></li>
                                    <li><a href="./blog-details.html">Produit</a></li>
                                </ul>
                            </li>
                            <li><a href="./blog.html">Blog</a></li>
                            <li><a href="./contact.html">Contact</a></li>
                        </ul>
                    </nav>
                </div>
                <div class="col-lg-3">
                    <div class="header__cart">
                        <ul>
                            <li><a href="#"><i class="fa fa-heart"></i> <span>1</span></a></li>
                            <li><a href="#"><i class="fa fa-shopping-bag"></i> <span>3</span></a></li>
                        </ul>
                        <div class="header__cart__price">item: <span>$150.00</span></div>
                    </div>
                </div>
            </div>
            <div class="humberger__open">
                <i class="fa fa-bars"></i>
            </div>
        </div>
    </header>
    <table border="1" class="table table-bordered border-primary table-primary">
        <tr>

            <th>Reference</th>
            <th>PrixBien</th>
            <th>Vendeur</th>
            <th>Client</th>
            <th>Date</th>
            <th>Agent</th>
        </tr>
        <tr {% for contrat in contrats %} >
            <td>{{ contrat.reference }}</td>
            <td>{{ contrat.prixBien }}</td>
            <td>
                {{ contrat.vendeur }}
            </td>
            <td>{{ contrat.client }}</td>
            <td>{{ contrat.date ? contrat.date|date('Y-m-d') : '' }}</td>
            <td>{{ contrat.agent }}</td>
        </tr>
        {% endfor %}
    </table>





</html>